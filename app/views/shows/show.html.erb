<div id="showCarousel" class="carousel slide" data-interval="false" data-ride="carousel">

  <div class="carousel-inner">
      <div class="carousel-item active image-show">
        <div class="d-block w-100" >
          <div>
            <%= cl_image_tag(@show.panels.first.wall.key, width: "100%", height: "100vh", class: "wall-img") %>
          </div>
        </div>
    </div>
    <div class="carousel-item image-show">
        <div class="d-block w-100" >
        <div>
            <%= cl_image_tag(@show.panels.first.wall.key, width: "100%", height: "100vh", class: "wall-img") %>
          </div>
    </div>
  </div>
  <a class="carousel-control-prev" href="#showCarousel" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#showCarousel" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
